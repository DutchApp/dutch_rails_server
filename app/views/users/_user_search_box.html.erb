<%= form_tag users_path, method: :get, class: 'form-search', id: 'user_search' do %>
    <%= text_field_tag :search, params[:search], class: 'search-query', autocomplete: 'off' %>
    <%= submit_tag "Search", class: 'btn', name: nil %>
<% end %>
<div class="search-dropdown span2" id="user_search_results"/>